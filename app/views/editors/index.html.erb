<p id="notice"><%= notice %></p>

<h1>Editors</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Date of Birth</th>
      <th>Email</th>
      <th colspan="4"></th>
    </tr>
  </thead>

  <tbody>
    <% @editors.each do |editor| %>
      <tr>
        <td><%= editor.first_name %></td>
        <td><%= editor.last_name %></td>
        <td><%= editor.dob %></td>
        <td><%= editor.email %></td>
        <td><%= link_to 'Show', editor %></td>
        <td><%= link_to 'Edit', edit_editor_path(editor) %></td>
        <td><%= link_to 'Destroy', editor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Editor', new_editor_path %>
